{
    "name": "MoneyPrinter_Production_Train",
    "email_notifications": {
        "no_alert_for_skipped_runs": false
    },
    "webhook_notifications": {},
    "timeout_seconds": 0,
    "max_concurrent_runs": 1,
    "tasks": [
        {
            "task_key": "train_model",
            "run_if": "ALL_SUCCESS",
            "spark_python_task": {
                "python_file": "jobs/train_remote.py",
                "parameters": [
                    "--epochs",
                    "50",
                    "--batch_size",
                    "32"
                ]
            },
            "environment_key": "Default",
            "timeout_seconds": 0,
            "email_notifications": {}
        }
    ],
    "environments": [
        {
            "environment_key": "Default",
            "spec": {
                "client": "1",
                "dependencies": [
                    "mlflow",
                    "torch",
                    "scikit-learn",
                    "pandas",
                    "numpy",
                    "joblib"
                ]
            }
        }
    ],
    "git_source": {
        "git_url": "https://github.com/Noah-kagyu-kinakomochi/democratic_money_printer.git",
        "git_provider": "gitHub",
        "git_branch": "main"
    },
    "format": "MULTI_TASK"
}